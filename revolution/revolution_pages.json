      "frame_to_load": "candidate_finder_home",
      "frame_contents_page": "candidate_search"
    }
  ],
  "pages": [
    {
      "class_name": "page",
      "page_name": "home",
      "default": true,
      "children": [
        {
          "class_name": "column",
          "box": {
            "width": "1.0 container",
            "height": "1.0 container"
          },
          "padding": "0 pixels",
          "content_alignment": "near",
          "content_cross_alignment": "center",
          "children": [
            {
              "class_name": "caption_bar",
              "corporate_name": "country video games",
              "image_file": "cockade.png",
              "title_name": "Candidate Research",
              "subtitle_name": "Country Video Games",
              "title_start": "0.28 container",
              "box": {
                "width": "1.0 container",
                "height": "100 pixels"
              }
            },
            {
              "class_name": "column",
              "box": {
                "width": "0.9 container",
                "height": "0.8 container"
              },
              "view_style": {
                "box_fill_brush": {
                  "class_name": "solid_brush",
                  "color": "313136"
                },
                "box_border_brush": {
                  "class_name": "solid_brush",
                  "color": "313136"
                },
                "box_border_thickness": 8
              },
              "padding": {
                "amount": 0,
                "units": "pixels"
              },
              "content_alignment": "near",
              "content_cross_alignment": "near",
              "children": [
                {
                  "class_name": "frame",
                  "name": "candidate_finder_home",
                  "box": {
                    "width": "1.0 container",
                    "height": "1.0 container"
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "class_name": "page",
      "page_name": "candidate_search",
      "children": [
        {
          "class_name": "column",
          "box": {
            "width": "1.0 container",
            "height": "1.0 container"
          },
          "padding": "0 pixels",
          "content_alignment": "near",
          "content_cross_alignment": "center",
          "children": [
            {
              "class_name": "row",
              "item_next_space": "16 pixels",
              "box": {
                "width": "1.0 container",
                "height": "1.0 container"
              },
              "children": [
                {
                  "class_name": "column",
                  "box": {
                    "width": "250 pixels",
                    "height": "1.0 container"
                  },
                  "children": [
                    {
                      "class_name": "chaptertitle",
                      "text": "Candidates",
                      "box": {
                        "width": "1.0 container",
                        "height": "30 pixels"
                      }
                    },
                    {
                      "class_name": "chaptersubtitle",
                      "text": "Search",
                      "box": {
                        "width": "1.0 container",
                        "height": "25 pixels"
                      }
                    },
                    {
                      "class_name": "form",
                      "name": "search_candidates_form",
                      "model": {
                        "name": "search_candidates_form",
                        "defaults": {
                          "box": {
                            "width": "1.0 container",
                            "height": "50 pixels"
                          },
                          "label_box": {
                            "width": "1.0 container",
                            "height": "25 pixels"
                          },
                          "field_box": {
                            "width": "1.0 remaining",
                            "height": "25 pixels"
                          }
                        },
                        "fields": [
                          {
                            "json_field_name": "Description",
                            "label_text": "Description",
                            "class_name": "edit",
                            "tooltip_text": "Search for a candidate",
                            "is_default_focus": true
                          }
                        ],
                        "data": []
                      },
                      "box": {
                        "width": "1.0 container",
                        "height": "75 pixels"
                      }
                    },
                    {
                      "class_name": "row",
                      "name": "event row",
                      "box": {
                        "width": "1.0 container",
                        "height": "50 pixels"
                      },
                      "item_next_space": "8 pixels",
                      "wrap": true,
                      "children": [
                        {
                          "class_name": "push_button",
                          "text": "Search",
                          "box": {
                            "width": {
                              "amount": 100,
                              "units": "pixels"
                            },
                            "height": {
                              "amount": 40,
                              "units": "pixels"
                            }
                          },
                          "click_command": {
                            "class_name": "search_objects_command",
                            "search_class_name": "Candidates",
                            "form_name": "search_candidates_form",
                            "table_name": "candidates_list",
                            "query": {
                              "class_name": "query_context",
                              "froms": {
                                "candidate_list": [],
                                "search_event_form": {}
                              },
                              "stages": [
                                {
                                  "stage_name": "search for any candidate text",
                                  "class_name": "filter",
                                  "stage_input_name": "Candidates:",
                                  "predicate": {
                                    "class_name": "contains",
                                    "textsrc": "search_candidates_form:Description"
                                  }
                                }
                              ]
                            }
                          }
                        }
                      ]
                    }
                  ]
                },
                {
                  "class_name": "column",
                  "box": {
                    "width": "0.50 container",
                    "height": "1.0 container"
                  },
                  "content_alignment": "near",
                  "content_cross_alignment": "near",
                  "children": [
                    {
                      "class_name": "chaptertitle",
                      "text": "",
                      "box": {
                        "width": "1.0 container",
                        "height": "30 pixels"
                      }
                    },
                    {
                      "class_name": "chaptersubtitle",
                      "text": "List",
                      "box": {
                        "width": "1.0 container",
                        "height": "25 pixels"
                      }
                    },
                    {
                      "class_name": "listview",
                      "name": "candidates_list",
                      "box": {
                        "width": "1.0 container",
                        "height": {
                          "amount": 1.0,
                          "units": "remaining"
                        }
                      },
                      "table": {
                        "id_field": "ObjectId",
                        "columns": [
                          {
                            "json_field_name": "FullName",
                            "label_text": "Full Name",
                            "width": 150
                          },
                          {
                            "json_field_name": "PoliticalParty",
                            "label_text": "Party",
                            "width": 150
                          },
                          {
                            "json_field_name": "CandidateId",
                            "label_text": "Id",
                            "width": 100
                          }
                        ],
                        "items": []
                      },
                      "select_command": {
                        "class_name": "preview_object_command",
                        "table_name": "candidates_list",
                        "form_name": "candidate_view_form"
                      }
                    }
                  ]
                },
                {
                  "class_name": "column",
                  "name": "candidate_view_column",
                  "box": {
                    "width": "1.0 remaining",
                    "height": "0.8 container"
                  },
                  "content_alignment": "near",
                  "content_cross_alignment": "near",
                  "children": [
                    {
                      "class_name": "chaptertitle",
                      "text": "",
                      "box": {
                        "width": "1.0 container",
                        "height": "30 pixels"
                      }
                    },
                    {
                      "class_name": "chaptersubtitle",
                      "text": "Edit",
                      "box": {
                        "width": "1.0 container",
                        "height": "25 pixels"
                      }
                    },
                    {
                      "class_name": "form",
                      "name": "candidate_view_form",
                      "model": {
                        "name": "edit_event",
                        "defaults": {
                          "box": {
                            "width": "1.0 container",
                            "height": "50 pixels"
                          },
                          "label_box": {
                            "width": "1.0 container",
                            "height": "25 pixels"
                          },
                          "field_box": {
                            "width": "1.0 remaining",
                            "height": "25 pixels"
                          }
                        },
                        "fields": [
                          {
                            "json_field_name": "FullName",
                            "label_text": "Full Name",
                            "class_name": "edit",
                            "tooltip_text": "Name of candidate",
                            "is_default_focus": true
                          },
                          {
                            "json_field_name": "PoliticalParty",
                            "label_text": "Party",
                            "class_name": "edit",
                            "tooltip_text": "Political Party",
                            "is_default_focus": false,
                            "field_box": {
                              "width": "250 pixels",
                              "height": "25 pixels"
                            },
                            "box": {
                              "width": "250 pixels",
                              "height": "50 pixels"
                            }
                          },
                          {
                            "json_field_name": "Story",
                            "label_text": "Story",
                            "class_name": "edit",
                            "tooltip_text": "The day of the event",
                            "is_default_focus": false,
                            "field_box": {
                              "width": "250 pixels",
                              "height": "125 pixels"
                            },
                            "box": {
                              "width": "250 pixels",
                              "height": "150 pixels"
                            }
                          }

                        ],
                        "data": []
                      },
                      "box": {
                        "width": "1.0 container",
                        "height": "250 pixels"
                      }
                    },
                    {
                      "class_name": "chaptersubtitle",
                      "text": "Create",
                      "box": {
                        "width": "1.0 container",
                        "height": "25 pixels"
                      }
                    },
                    {
                      "class_name": "push_button",
                      "text": "Save",
                      "box": {
                        "width": {
                          "amount": 100,
                          "units": "pixels"
                        },
                        "height": {
                          "amount": 40,
                          "units": "pixels"
                        }
                      },
                      "click_command": {
                        "class_name": "save_object_command",
                        "form_name": "candidate_view_form"
                      }
                    }

                  ]
                }
